<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">

 <context:annotation-config />
	<!-- using Annotations -->
	<bean id="employee" class="com.prowings.beanlifecycle.Employee" autowire="byType">
		<property name="id" value="85"></property>
		<property name="name" value="Suresh"></property>
	</bean>

	<!-- using Annotations -->


	<bean id="department" class="com.prowings.beanlifecycle.Department">
		<property name="deptId" value="97"></property>
		<property name="deptName" value="Development"></property>
	</bean>

</beans>